ngx_addon_name=ngx_http_redis_module

REDIS_SRCS="                                                         \
                $ngx_addon_dir/src/ngx_http_redis_module.c            \
                "

REDIS_DEPS="                                                         \
                $ngx_addon_dir/src/ngx_http_redis_module.h            \
                "

if test -n "$ngx_module_link"; then
    ngx_module_type=HTTP
    ngx_module_name=$ngx_addon_name
    ngx_module_incs=
    ngx_module_deps="$REDIS_DEPS"
    ngx_module_srcs="$REDIS_SRCS"
    ngx_module_libs=

    . auto/module
else
    HTTP_MODULES="$HTTP_MODULES $ngx_addon_name"
    NGX_ADDON_SRCS="$NGX_ADDON_SRCS $REDIS_SRCS"
    NGX_ADDON_DEPS="$NGX_ADDON_DEPS $REDIS_DEPS"
fi

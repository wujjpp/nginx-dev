ngx_addon_name=ngx_http_example_access_module

EXAMPLE_ACCESS_SRCS="                                                         \
                $ngx_addon_dir/src/ngx_http_example_access_module.c            \
                $ngx_addon_dir/src/ngx_http_example_access_handler.c                    \
                "

EXAMPLE_ACCESS_DEPS="                                                          \
                $ngx_addon_dir/src/ddebug.h                                      \
                $ngx_addon_dir/src/ngx_http_example_access_module.h              \
                $ngx_addon_dir/src/ngx_http_example_access_handler.h                      \
                "

if test -n "$ngx_module_link"; then
    ngx_module_type=HTTP
    ngx_module_name=$ngx_addon_name
    ngx_module_incs=
    ngx_module_deps="$EXAMPLE_ACCESS_DEPS"
    ngx_module_srcs="$EXAMPLE_ACCESS_SRCS"
    ngx_module_libs=

    . auto/module
else
    HTTP_MODULES="$HTTP_MODULES $ngx_addon_name"
    NGX_ADDON_SRCS="$NGX_ADDON_SRCS $EXAMPLE_ACCESS_SRCS"
    NGX_ADDON_DEPS="$NGX_ADDON_DEPS $EXAMPLE_ACCESS_DEPS"
fi
